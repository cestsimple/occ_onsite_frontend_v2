import request from '@/utils/request'

export function getInvoiceDiff(params) {
  // 返回promise对象
  return request({
    url: '/monthly/variable/diff/',
    method: 'get',
    params
  })
}

export function updateInvoiceDiff(data) {
  // 返回promise对象
  return request({
    url: `/monthly/variable/diff/${data.id}/`,
    method: 'put',
    data
  })
}
